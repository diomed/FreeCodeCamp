{
  "name": "markdownprieviewer",
  "version": "1.0.0",
  "description": "A live markdown previewer written in react",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build-css": "node-sass --output-style compressed --include-path sass sass/main.sass src/App.min.css",
    "watch-css": "node-sass -w --output-style compressed --include-path sass sass/main.sass src/App.min.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "markdown",
    "react",
    "freecodecamp"
  ],
  "author": "Kent Delante",
  "license": "GPL-3.0",
  "dependencies": {
    "marked": "^0.3.6",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "node-sass": "^4.5.3",
    "react-scripts": "^1.0.7"
  }
}
